<section class="invoice" id="planView">
    <div class="page-header text-center">
        Plan Name
        <button type="button" id="backPlan" class="btn btn-default pull-right">Back</button>
    </div>
    
    <div class="row">
        <div class="col-md-6">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Plan Details</h3>
                </div>
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table">
                            <tbody>
                                <tr>
                                    <th>Start Date</th>
                                    <td>25-Jan-2018</td>
                                </tr>
                                <tr>
                                    <th>Monthly Investment</th>
                                    <td>
                                        <span>
                                            <i class="fa fa-dollar"></i> 25000
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <th>Periodicity</th>
                                    <td>Monthly</td>
                                </tr>
                                <tr>
                                    <th>Cycles</th>
                                    <td>12</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
        <div class="col-md-6 invoice-col">
            <div class="box box-info">
                <div class="box-header with-border">
                    <h3 class="box-title">Summary</h3>
                </div>
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table">
                            <tbody>
                                <tr>
                                    <th style="width:50%">Total Paid </th>
                                    <td>
                                        <span>
                                            <i class="fa fa-dollar"></i> 75,000
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <th>Total Earned</th>
                                    <td>
                                        <span>
                                            <i class="fa fa-dollar"></i>
                                            12,512
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <th>Withdrawal Status</th>
                                    <td>Yes</td>
                                </tr>
                                <tr>
                                    <th>Withdrawal Details</th>
                                    <td>
                                        <i class="fa fa-dollar"></i> 2,63,000 
                                        on 24-Mar-2018
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="box-footer">
                    <button type="button" id="detailPlan" class="btn btn-default pull-right">View Details
                    </button>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            <div class="box box-success">
                <div class="box-header with-border">
                    <h3 class="box-title">Payment</h3>
                </div>
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table">
                            <tbody>
                                <tr>
                                    <th>Due Date</th>
                                    <td>23-Mar-2018  17:00</td>
                                </tr>
                                <tr>
                                    <th>Amount</th>
                                    <td>
                                        <span>
                                            <i class="fa fa-dollar"></i> 22,000
                                        </span>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="box-footer">
                    <button type="button" class="btn btn-default pull-right">
                        Pay
                    </button>
                </div>
            </div>
        </div>

        <div class="col-md-6 invoice-col">
            <div class="box box-warning">
                <div class="box-header with-border">
                    <h3 class="box-title">Withdraw Request</h3>
                </div>
                <div class="box-body">
                    <div class="table-responsive">
                        <table class="table">
                            <tbody>
                                <tr>
                                    <th>Max Withdrawable Amt</th>
                                    <td>
                                        <span>
                                            <i class="fa fa-dollar"></i>
                                            2,75,000
                                        </span>
                                    </td>
                                </tr>
                                <tr>
                                    <th>Your Ask/Bid :</th>
                                    <td>
                                        <input type="text" id="bid-txt" class="form-control" value="271000"/>
                                        <p class="">Closes on 
                                            <span>25-Mar-2018 17:00</span>
                                            <div>14 Bidders, Min Bid : <i class="fa fa-dollar"></i> 2,68,500</div>
                                        </p>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="box-footer">
                    <button type="button" onclick="bid()" class="btn btn-default pull-right">Bid
                    </button>
                </div>
            </div>
        </div>
    </div>

</section>

